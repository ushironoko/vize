{
  "name": "@vizejs/fresco-native",
  "version": "0.0.1-alpha.102",
  "description": "Vue TUI framework - Native bindings",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "vize-fresco",
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-pc-windows-msvc",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl"
    ]
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "build": "napi build --platform --release --manifest-path ../../crates/vize_fresco/Cargo.toml -p vize_fresco --features napi --output-dir .",
    "build:debug": "napi build --platform --manifest-path ../../crates/vize_fresco/Cargo.toml -p vize_fresco --features napi --output-dir .",
    "prepublishOnly": "napi pre-publish -t npm --no-gh-release --skip-optional-publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/vize"
  },
  "keywords": [
    "vue",
    "tui",
    "terminal",
    "native",
    "napi",
    "vize",
    "fresco"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0"
  }
}
