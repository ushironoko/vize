{
  "name": "@vizejs/nuxt",
  "version": "0.0.1-alpha.102",
  "description": "Nuxt module for Vize - compiler, musea gallery, linter, and type checker",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "vue",
    "vize",
    "compiler",
    "musea",
    "gallery",
    "linter",
    "type-checker"
  ],
  "author": "ubugeeei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/vize.git",
    "directory": "npm/nuxt"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.0",
    "@vizejs/vite-plugin": "workspace:^",
    "@vizejs/vite-plugin-musea": "workspace:^",
    "@vizejs/musea-nuxt": "workspace:^",
    "vize": "workspace:^"
  },
  "devDependencies": {
    "tsdown": "^0.9.0",
    "typescript": "^5.7.0",
    "vite": "^8.0.0-beta.0",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
