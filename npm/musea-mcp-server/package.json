{
  "name": "@vizejs/musea-mcp-server",
  "version": "0.0.1-alpha.78",
  "description": "MCP server for building Vue.js design systems - component analysis, documentation, variant generation, and design tokens",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "musea-mcp": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "start": "node dist/cli.js",
    "lint": "oxlint --deny-warnings --type-aware --tsconfig tsconfig.json",
    "lint:fix": "oxlint --type-aware --tsconfig tsconfig.json --fix",
    "fmt": "oxfmt --write src",
    "fmt:check": "oxfmt src"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "vue",
    "design-system",
    "component-analysis",
    "musea",
    "ai"
  ],
  "author": "ubugeeei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ubugeeei/vize.git",
    "directory": "npm/musea-mcp-server"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@vizejs/native": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "tsdown": "^0.9.0",
    "typescript": "^5.7.0"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  }
}
