{
  "name": "vizejs",
  "version": "0.0.1-alpha.11",
  "description": "High-performance Vue.js compiler - Main package",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "compiler",
    "vize",
    "config"
  ],
  "license": "MIT",
  "dependencies": {
    "esbuild": "^0.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "~5.6.0"
  }
}
