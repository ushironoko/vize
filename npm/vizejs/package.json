{
  "name": "vizejs",
  "version": "0.0.1-alpha.47",
  "description": "High-performance Vue.js compiler - Main package",
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./config": {
      "import": "./dist/config.js",
      "types": "./dist/config.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint --deny-warnings --type-aware --tsconfig tsconfig.json",
    "lint:fix": "oxlint --type-aware --tsconfig tsconfig.json --fix",
    "fmt": "oxfmt --write src",
    "fmt:check": "oxfmt src"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "compiler",
    "vize",
    "config"
  ],
  "license": "MIT",
  "dependencies": {
    "oxc-transform": "^0.56.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsdown": "^0.9.0",
    "typescript": "~5.6.0"
  }
}
